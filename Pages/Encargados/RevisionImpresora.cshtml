@page
@model Impresoras3D.App.Frontend.Pages.RevisionImpresoraModel
@{
    ViewData["Title"] = "Adicionar Revision";
    //int ImpresoraId = Model.impresora3D.Id;
}
<br><p>@TempData["Usuario"]</p>
<h1>Adicionar</h1>
<h4>Revision</h4>
<h3>@ViewData["Error"]</h3>
<div class="form-group">
    <label for="impresora3D">PlacaInventario</label>
    <input class="form-control" type="text" value="@Model.impresora3D.PlacaInventario"aria-label="readonly input example"readonly>
    <label for="impresora3D">Id</label>
    <input class="form-control" type="text" value="@Model.impresora3D.Id"aria-label="readonly input example"readonly>
   
   

</div>


<div class="form-horizontal">
    <hr />
    <div class="row">
        <div class="col-md-4">
            <h3>@ViewData["Error"]</h3>
            <form method="POST">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <div class="form-group">
                    <div class="form-group col-sm-6">
                        <label asp-for="revision.EstadoCabezal"></label>
                        <select asp-for="revision.EstadoCabezal" class="form-control">
                            <option value="" disabled="" selected="">Selecciona el estado de cabezal</option>
                            <option value="Correcto">Correcto</option>
                            <option value="Defectuoso">Defectuoso</option>
                            <option value="Proximo a cambiar">Proximo a cambiar</option>
                        </select>
                        <span asp-validation-for="revision.EstadoCabezal" class="text-danger"></span>
                    </div>
                    <div class="form-group col-sm-6">
                        <label asp-for="revision.EstadoExtrusor"></label>
                        <select asp-for="revision.EstadoExtrusor" class="form-control">
                            <option value="" disabled="" selected="">Selecciona el estado de extrusor</option>
                            <option value="Correcto">Correcto</option>
                            <option value="Defectuoso">Defectuoso</option>
                            <option value="Proximo a cambiar">Proximo a cambiar</option>
                        </select>
                        <span asp-validation-for="revision.EstadoExtrusor" class="text-danger"></span>
                    </div>
                    <div class="form-group col-sm-6">
                        <label asp-for="revision.VoltajeFuente" class="control-label">Voltaje de Fuente </label>
                        <input type="text" asp-for="revision.VoltajeFuente" class="form-control" />
                        <span asp-validation-for="revision.VoltajeFuente" class="text-danger"></span>
                    </div>
                    <div class="form-group col-sm-6">
                        <label asp-for="revision.AlineacionCama"></label>
                        <select asp-for="revision.AlineacionCama" class="form-control">
                            <option value="" disabled="" selected="">Selecciona el estado de la alineacion de la cama</option>
                            <option value="Alineada">Alineada</option>
                            <option value="Desalineada">Desalineada</option>
                            <option value="Rota">Rota</option>
                        </select>
                        <span asp-validation-for="revision.AlineacionCama" class="text-danger"></span>
                    </div>
                    <div class="form-group col-sm-6">
                        <label asp-for="revision.FechaRevision" class="control-label">FechaRevision </label>
                        <input  asp-for="revision.FechaRevision" class="form-control" />
                        <span asp-validation-for="revision.FechaRevision" class="text-danger"></span>
                    </div>
                   
                    @*<div class="form-group col-sm-6">
                        <label asp-for="revision.Impresora3DId" class="control-label">impresora </label>
                        <input type="text" asp-for="revision.Impresora3DId" class="form-control" />
                        <span asp-validation-for="revision.Impresora3DId" class="text-danger"></span>
                       <input type="text" asp-for="@Model.impresora3D.Id" class="form-control" />
                    </div>
                    @*<div class="form-group col-sm-6">
                        <label asp-for="impresora3D.Id" class="control-label">impresora </label>
                        <input type="text" asp-for="impresora3D.Id" class="form-control" />
                        <span asp-validation-for="impresora3D.Id" class="text-danger"></span>
                    </div>*@
                </div><br>
                <div class="form-group">
                    <input type="submit" value="Guardar" class="btn btn-primary" />
                </div>
            </form>
        </div>
    </div>
</div>


